<h1>Profile Page</h1>
<hr>
<br><br>

<h2>Your Information:</h2>
<div>
<div id="userInformationTableDiv">
<table id="userInformationTable">
  <tr>
    <td>Name</td>
    <td>{{user.firstName}} {{user.lastName}}</td>
  </tr>
  <tr>
    <td>Email Address</td>
    <td>{{user.emailAddress}}</td>
  </tr>
  <tr>
    <td>Phone Number</td>
    <td>{{user.phoneNumber}}</td>
  </tr>
</table>
</div>
<div>
{{#if user.image}}
<img src="http://localhost:3000/data/uploads/{{user.image}}" alt="">
{{/if}}
</div>
</div>
<br><br>

<h2>Your Listings:</h2>
<ul>
  {{#each listings}}
    <li>
      <h3>{{this.title}}</h3>
      <p>{{this.description}}</p>
      <p>{{this.price}}</p>
    </li>
  {{/each}}
</ul>
<br><br>

<a href="listing/add">Make new listing</a>
<br>
<a href="homepage">Homepage</a>
